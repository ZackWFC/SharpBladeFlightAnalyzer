<Window x:Class="SharpBladeFlightAnalyzer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SharpBladeFlightAnalyzer"
        mc:Ignorable="d"
        Title="MainWindow" Height="700" Width="1000" MinHeight="700" MinWidth="1000">
	<Window.Resources>

	</Window.Resources>
	<Grid Background="Gray">
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="200" MinWidth="200" MaxWidth="500"/>
			<ColumnDefinition Width="3"/>
			<ColumnDefinition Width="1*"/>
		</Grid.ColumnDefinitions>
		<ListView Background="Red"/>

		<GridSplitter x:Name="gridSplitter" Grid.Column="1" Width="3" HorizontalAlignment="Center"/>
		<TabControl Grid.Column="2">
			<local:TabPage Header="New">
				
			</local:TabPage>
            <local:TabPage Header="New2"/>
            <local:TabPage Header="New3"/>
            <local:TabPage Header="New4"/>
            <local:TabPage Header="New4"/>
            <local:TabPage Header="New4"/>
        </TabControl>
	</Grid>
</Window>
